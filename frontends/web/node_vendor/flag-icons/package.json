{
  "name": "flag-icons",
  "version": "6.6.6",
  "author": "Panayiotis Lipiridis <lipiridis@gmail.com>",
  "license": "MIT",
  "main": "css/flag-icons.css",
  "repository": {
    "type": "git",
    "url": "http://github.com/lipis/flag-icons"
  },
  "scripts": {
    "start": "npm run svgo:all",
    "ids": "python3 flag-ids.py",
    "svgo:all": "find flags | grep '\\.svg$' | xargs -Iz -n 1 npm run svgo z && npm run ids",
    "svgo:min": "find flags | grep '\\.svg$' | xargs -Iz -n 1 svgo z && npm run ids",
    "svgo": "svgo --pretty --indent=2 --precision=0",
    "test": "yarn prettier --list-different"
  }
}

{
  "private": true,
  "name": "preact2",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npx tcm src --camelCase dashes",
    "build": "preact build --no-prerender --service-worker=false",
    "dev": "preact watch",
    "lint": "eslint --max-warnings 0 --cache --ignore-path ../../.gitignore '**/*.{js,jsx}'",
    "postlint": "tslint -e 'src/**/*.css.d.ts' 'src/**/*.ts?(x)'",
    "test": "jest",
    "browserlist": "npx browserslist"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@types/jest": "23.3.10",
    "babel-plugin-module-resolver": "3.1.1",
    "eslint": "5.1.0",
    "eslint-config-synacor": "3.0.1",
    "identity-obj-proxy": "3.0.0",
    "if-env": "1.0.4",
    "jest": "23.6.0",
    "preact-cli": "2.2.1",
    "preact-cli-plugin-env-vars": "^1.2.1",
    "preact-cli-plugin-typescript": "0.2.2",
    "preact-render-spy": "1.3.0",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "typed-css-modules": "0.3.5",
    "typings-for-css-modules-loader": "1.7.0"
  },
  "dependencies": {
    "@zxing/library": "0.11.0",
    "i18next": "11.3.6",
    "i18next-locize-backend": "1.6.0",
    "linkstate": "1.1.1",
    "locize-editor": "1.7.0",
    "preact": "8.5.2",
    "preact-compat": "3.18.0",
    "preact-router": "2.6.1",
    "react-i18next": "7.9.0",
    "sortablejs": "^1.10.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}

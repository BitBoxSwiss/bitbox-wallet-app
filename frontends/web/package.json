{
  "private": true,
  "name": "preact2",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npx tcm src --camelCase dashes",
    "build": "preact build --no-prerender --service-worker=false",
    "dev": "preact watch",
    "lint": "eslint --cache --ignore-path ../../.gitignore \"**/*.{js,jsx}\"",
    "browserlist": "npx browserslist"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "babel-plugin-module-resolver": "3.1.1",
    "eslint": "5.1.0",
    "eslint-config-synacor": "3.0.1",
    "if-env": "1.0.4",
    "preact-cli": "2.2.1",
    "preact-cli-plugin-typescript": "0.2.2",
    "typings-for-css-modules-loader": "1.7.0",
    "typed-css-modules": "0.3.5"
  },
  "dependencies": {
    "i18next": "11.3.6",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "preact-router": "2.6.1",
    "react-i18next": "7.9.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
